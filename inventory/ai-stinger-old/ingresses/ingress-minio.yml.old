apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: minio-ingress
  namespace: minio
  annotations:
    kubernetes.io/ingress.class: nginx
spec:
  rules:
    - host: do-nitro-1.one.den.solidfire.net
      http:
        paths:
        - path: /minio
          backend:
            serviceName: minio
            servicePort: 9000
  tls:
    - hosts:
      - do-nitro-1.one.den.solidfire.net
      secretName: minio-tls
